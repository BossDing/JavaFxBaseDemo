<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxHeight="500.0" maxWidth="1000.0" minHeight="500.0" minWidth="1000.0" prefHeight="500.0" prefWidth="1000.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hospital.LoginController">
   <stylesheets>
        <URL value="@base.css" />
    </stylesheets>
   <children>
      <GridPane layoutX="359.0" layoutY="193.0" styleClass="login_grid">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="10.0" prefWidth="136.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="96.0" minWidth="10.0" prefWidth="64.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="用户名：" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="username" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Label text="密   码：" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <PasswordField fx:id="password" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <HBox prefHeight="100.0" prefWidth="200.0" styleClass="h_box" GridPane.columnIndex="2" GridPane.rowIndex="3">
               <children>
                  <Button mnemonicParsing="false" onAction="#gotoMainPageAction" styleClass="button" text="登陆">
                     <HBox.margin>
                        <Insets right="5.0" top="5.0" />
                     </HBox.margin></Button>
                  <Button mnemonicParsing="false" onAction="#handleButtonAction" styleClass="button" text="取消">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
    
</AnchorPane>
